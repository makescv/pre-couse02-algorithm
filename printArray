// Input: Array of elements

// ["h","o","l","a"]

// Output: String with comma delimited elements of the array in th same order.

// "h,o,l,a"

function printArray(array){
  var ret="\"";
  //show me the code!
  for (var i=0;i<array.length;++i){
  	ret+=array[i];
  	if (i<array.length-1)
  		ret+=",";
  }
  return ret+"\"";
}
printArray(["h","o","l","a"]);